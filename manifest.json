{
    "manifest_version": 3,
    "name": "ULookUp",
    "version": "1.0",
    "description": "Usefull Lookup, lookup any selected text in a page with an LLM",
    "permissions": ["contextMenus", "activeTab", "notifications", "tabs", "storage"],
    "icons": {
        "16": "icons-16.png",
        "32": "icons-32.png",
        "48": "icons-48.png",
        "128": "icons-128.png"
      },
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
    "default_popup": "hello.html",
      "default_icon": {
        "16": "icons-16.png",
        "32": "icons-32.png",
        "48": "icons-48.png",
        "128": "icons-128.png"
      }
    }
  }